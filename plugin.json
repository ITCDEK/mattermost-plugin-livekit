{
    "id": "com.mattermost.plugin-livekit",
    "name": "VoiceMatters",
    "description": "LiveKit audio and video conferencing plugin for Mattermost",
    "homepage_url": "https://github.com/ITCDEK/mattermost-plugin-livekit",
    "support_url": "https://github.com/ITCDEK/mattermost-plugin-livekit/issues",
    "release_notes_url": "https://github.com/ITCDEK/mattermost-plugin-livekit/releases/tag/v0.1.0",
    "icon_path": "assets/bot-icon.svg",
    "version": "0.1.0",
    "min_server_version": "5.37.0",
    "server": {
        "executables": {
            "linux-amd64": "server/dist/plugin-linux-amd64",
            "linux-arm64": "server/dist/plugin-linux-arm64",
            "darwin-amd64": "server/dist/plugin-darwin-amd64",
            "darwin-arm64": "server/dist/plugin-darwin-arm64",
            "windows-amd64": "server/dist/plugin-windows-amd64.exe"
        }
    },
    "webapp": {
        "bundle_path": "webapp/dist/main.js"
    },
    "settings_schema": {
        "header": "For the detailed settings description, please visit https://github.com/ITCDEK/mattermost-plugin-livekit",
        "footer": "VoiceMatters settings",
        "settings": [
            {
                "type": "text",
                "key": "Server1",
                "display_name": "Server #1",
                "help_text": "JSON-formatted string, see plugin documentation on GitHub",
                "placeholder": "{host: 'livekit.on.me', port: 7880}",
                "default": null
            }
        ]
    }
}
