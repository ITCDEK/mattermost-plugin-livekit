{
    "id": "com.mattermost.plugin-livekit",
    "name": "VoiceMatters",
    "description": "LiveKit audio and video conferencing plugin for Mattermost",
    "homepage_url": "https://github.com/ITCDEK/mattermost-plugin-livekit",
    "support_url": "https://github.com/ITCDEK/mattermost-plugin-livekit/issues",
    "release_notes_url": "https://github.com/ITCDEK/mattermost-plugin-livekit/releases/tag/v0.1.0",
    "icon_path": "assets/bot-icon.svg",
    "version": "0.1.0",
    "min_server_version": "5.37.0",
    "server": {
        "executables": {
            "linux-amd64": "server/dist/plugin-linux-amd64",
            "linux-arm64": "server/dist/plugin-linux-arm64",
            "darwin-amd64": "server/dist/plugin-darwin-amd64",
            "darwin-arm64": "server/dist/plugin-darwin-arm64",
            "windows-amd64": "server/dist/plugin-windows-amd64.exe"
        }
    },
    "webapp": {
        "bundle_path": "webapp/dist/main.js"
    },
    "settings_schema": {
        "settings": [
            {
                "type": "text",
                "key": "host",
                "display_name": "Host",
                "placeholder": "livekit.our.own"
            },
            {
                "type": "number",
                "key": "port",
                "display_name": "Host port",
                "placeholder": "7880"
            },
            {
                "type": "bool",
                "key": "secure",
                "display_name": "Host over HTTPS"
            },
            {
                "type": "text",
                "key": "turnhost",
                "display_name": "TURN Host",
                "placeholder": "turn.our.own"
            },
            {
                "type": "number",
                "key": "turnport",
                "display_name": "Turn TCP port",
                "placeholder": "5349"
            },
            {
                "type": "number",
                "key": "turnudp",
                "display_name": "Turn UDP port",
                "placeholder": "443"
            },
            {
                "type": "bool",
                "key": "turnsecure",
                "display_name": "TURN over HTTPS"
            },
            {
                "type": "text",
                "key": "apikey",
                "display_name": "API secret key",
                "placeholder": "API7vTUvag3wqvW"
            },
            {
                "type": "text",
                "key": "apivalue",
                "display_name": "API secret value",
                "placeholder": "mqtMUyGxMzzw7tUfGmlYIo4utTs66svftv8MRh1HTxp"
            }
        ],
        "footer": "For the detailed settings description, please visit https://github.com/ITCDEK/mattermost-plugin-livekit"
    }
}
